{
  "folders": [
    {
      "name": "[Repository]",
      "path": "."
    }
  ],
  "settings": {
    "breadcrumbs.enabled": true,
    "editor.autoIndent": "full",
    "editor.fontSize": 18,
    "editor.formatOnSave": true,
    "editor.renderWhitespace": "all",
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "explorer.autoReveal": true,
    "explorer.sortOrder": "type",
    "extensions.ignoreRecommendations": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "files.autoSave": "onFocusChange",
    "files.eol": "\n",
    "files.exclude": {
      "**/.classpath": true,
      "**/.project": true,
      "**/.settings": true,
      "**/.factorypath": true
    },
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.format.enabled": false,
    "python.formatting.blackArgs": ["--line-length", "120"],
    "python.formatting.provider": "black",
    "python.linting.flake8Args": ["--max-line-length=120"],
    "python.linting.flake8Enabled": true,
    "python.linting.mypyArgs": [],
    "python.linting.mypyEnabled": true,
    "python.linting.pylintArgs": [],
    "python.linting.pylintEnabled": true,
    "python.pythonPath": "~/.pyenv/versions/default/",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "window.title": "${activeEditorMedium}${separator}${rootName}${separator}${rootPath}",
    "window.zoomLevel": 0,
    /*"workbench.colorCustomizations": {
      "editorWhitespace.foreground": "#FF0000",
    },*/
    "workbench.colorTheme": "Noctis Azureus",
    "workbench.editor.enablePreview": false,
    "workbench.tree.indent": 24,
    "[dockerfile]": {
      "editor.formatOnSave": false,
      "editor.insertSpaces": true,
      "editor.tabSize": 4
    },
    "[groovy]": {
      "editor.insertSpaces": true,
      "editor.tabSize": 2
    },
    "[java]": {
      "editor.tabSize": 2,
      "editor.insertSpaces": true
    },
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.insertSpaces": true,
      "editor.tabSize": 2
    },
    "[javascriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.insertSpaces": true,
      "editor.tabSize": 2
    },
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.insertSpaces": true,
      "editor.tabSize": 2
    },
    "[python]": {
      "editor.tabSize": 4,
      "editor.insertSpaces": true
    },
    "[terraform]": {
      "editor.insertSpaces": true,
      "editor.tabSize": 2
    },
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.insertSpaces": true,
      "editor.tabSize": 2
    },
    "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.insertSpaces": true,
      "editor.tabSize": 2
    },
    "[makefile]": {
      "editor.insertSpaces": false,
      "editor.tabSize": 2
    },
    "[markdown]": {
      "editor.insertSpaces": true,
      "editor.tabSize": 2
    },
    "[xml]": {
      "editor.insertSpaces": true,
      "editor.tabSize": 2
    },
    "[yaml]": {
      "editor.insertSpaces": true,
      "editor.tabSize": 2
    },
    "emeraldwalk.runonsave": {
      "commands": [
        {
          "match": "\\.java$",
          "cmd": "/usr/local/bin/google-java-format --replace ${file}"
        }
      ]
    },
    "workbench.colorCustomizations": {
      "activityBar.activeBackground": "#7f40bf",
      "activityBar.activeBorder": "#c78f58",
      "activityBar.background": "#7f40bf",
      "activityBar.foreground": "#e7e7e7",
      "activityBar.inactiveForeground": "#e7e7e799",
      "activityBarBadge.background": "#c78f58",
      "activityBarBadge.foreground": "#15202b",
      "statusBar.background": "#663399",
      "statusBar.border": "#663399",
      "statusBar.foreground": "#e7e7e7",
      "statusBarItem.hoverBackground": "#7f40bf",
      "titleBar.activeBackground": "#663399",
      "titleBar.activeForeground": "#e7e7e7",
      "titleBar.border": "#663399",
      "titleBar.inactiveBackground": "#66339999",
      "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#639"
  }
}
